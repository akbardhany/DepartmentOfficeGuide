<?xml version='1.0' encoding='utf-8'?>
<widget id="ac.id.narotama" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>D.O. Guide</name>
    <description>
        Hopefully u're happy.
    </description>
    <author email="dhanydhuny@gmail.com" href="https://github.com/akbardhany">
        scrupscrup
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="hdpi" platform="android" src="res/screen/android/Logo-Surabaya.png" />
        <splash density="hdpi" src="res/screen/android/Logo-Surabaya.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-splashscreen" spec="https://github.com/apache/cordova-plugin-splashscreen.git" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
